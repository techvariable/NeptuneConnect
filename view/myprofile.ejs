<!DOCTYPE html>
<html dir="ltr" lang="en">
  <head>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=5.0"
    />
    <title>Neptune Connect</title>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700&display=swap"
      rel="stylesheet"
    />

    <!-- <script src="https://cdn.tailwindcss.com"></script> -->

    <link rel="stylesheet" href="/public/assets/css/style.css" />
    <link rel="stylesheet" href="/public/components/build/neptune.css" />

    <script type="module" src="/public/components/build/neptune.esm.js"></script>
    <script nomodule src="/public/components/build/neptune.js"></script>
  </head>

  <body
    class="selection:bg-indigo-200 selection:text-violet-800 relative font-roboto w-100 h-screen bg-white"
  >
    <% if(mode==='demo'){ %>
    <banner-component></banner-component>
    <% } %>
    <fluid-container>
      <nav-bar stringifieduser="<%= user %>" imgurl="<%=imgurl%>"></nav-bar>
      <div class="container mx-auto md:flex gap-4 mt-4">
        <div class="w-full md:flex justify-center gap-4">
          <aside-container>
            <aside class="w-full" aria-label="Sidebar">
              <navigators-component
                navigators="<%=JSON.stringify(navigators) %>"
                permissions="<%= JSON.stringify(permissions) %>"
              ></navigators-component>
            </aside>
          </aside-container>
          <div class="flex-grow">
            <h2 class="pb-1.5 font-mono text-lg font-bold leading-7 text-gray-600">Profile</h2>
            <profile-component
              url="<%= url %>"
              stringifieduser="<%= user %>"
              permissions="<%= JSON.stringify(permissions) %>"
            ></profile-component>
            <h2 class="pt-2 pb-2 font-mono text-lg font-bold leading-7 text-gray-600">
              Aggregated Permissions
            </h2>
            <permission-table
              permissionstring="<%= JSON.stringify(permissions) %>"
            ></permission-table>
          </div>
        </div>
      </div>
    </fluid-container>
    <script src="/public/scripts/settingScript.js"></script>
  </body>
</html>
